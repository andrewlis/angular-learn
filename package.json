    "@angular/router": "~12.1.0",
    "@ngxs/router-plugin": "^3.7.3",
    "@ngxs/storage-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "json-server": "^0.17.0",
    "ngx-socket-io": "^4.2.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.0",
